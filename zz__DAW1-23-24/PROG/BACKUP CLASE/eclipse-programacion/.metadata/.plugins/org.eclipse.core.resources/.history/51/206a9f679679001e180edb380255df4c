package ejemplosBucles;

public class Ejemplo09 {

	public static void main(String[] args) {

		//El método Math.random() devuelve un número mayor o igual a 0 y 
		// menor que 1.
	
		int contador = 1;
		while (contador <= 10) {
			
			double x = ((Math.random()) * 2 ) + 1;
			System.out.println(x);
			contador++;
			
		}

	}

}
