package hoja02;

import java.util.Scanner;

public class Ejer22 {

	public static void main(String[] args) {

		Scanner entrada = new Scanner(System.in);
		
		System.out.println("LAS TABLAS DE MULTIPLICAR DEL 1 AL 10");
		
		for(int i = 0; i <= 10; i++) {
	
			System.out.println(i * i + "\n");
	
		}

	}

}
